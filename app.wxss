/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 公共部分 */
.titleBar{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.titleBar .title{
  padding-left: 20rpx;
  border-left:2px solid #ff6500;
  font-size: 45rpx;
  font-weight: bold;
  color: #000;
}
.titleBar .more{
  font-size: 30rpx;
  color: #777;
  padding-right:20rpx ;
}
